<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="550.0" minWidth="500.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dealsMatured.dealsMaturedViewController">
   <children>
      <Label layoutX="115.0" layoutY="23.0" prefHeight="36.0" prefWidth="231.0" text="     Deals Matured">
         <font>
            <Font name="Cambria Bold" size="29.0" />
         </font>
      </Label>
      <Label layoutX="29.0" layoutY="91.0" prefHeight="26.0" prefWidth="150.0" text="Buyer ID/Contact">
         <font>
            <Font name="Cambria Bold" size="18.0" />
         </font>
      </Label>
      <ComboBox fx:id="comboBuyerID" layoutX="29.0" layoutY="131.0" onAction="#docomboBuyer" prefWidth="150.0" />
      <Label layoutX="300.0" layoutY="91.0" prefHeight="26.0" prefWidth="150.0" text="Seller ID/Mobile">
         <font>
            <Font name="Cambria Bold" size="18.0" />
         </font>
      </Label>
      <ComboBox fx:id="comboSellerID" layoutX="300.0" layoutY="131.0" onAction="#docomboSeller" prefWidth="150.0" />
      <Label layoutX="29.0" layoutY="180.0" text="Property ID">
         <font>
            <Font name="Cambria Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="300.0" layoutY="180.0" text="Total Amount">
         <font>
            <Font name="Cambria Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="txtTotalAmount" layoutX="300.0" layoutY="221.0" />
      <Label layoutX="29.0" layoutY="265.0" text="Advance Payment">
         <font>
            <Font name="Cambria Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="txtAdvanceAmount" layoutX="29.0" layoutY="310.0" />
      <Label layoutX="299.0" layoutY="265.0" text="Balence Amount">
         <font>
            <Font name="Cambria Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="txtBalAmount" layoutX="301.0" layoutY="310.0" onKeyPressed="#doBalence" />
      <Label layoutX="72.0" layoutY="361.0" prefHeight="26.0" prefWidth="359.0" text="Date Of Registration / Ownership Transfer">
         <font>
            <Font name="Cambria Bold" size="18.0" />
         </font>
      </Label>
      <DatePicker fx:id="dateReg" layoutX="132.0" layoutY="402.0" />
      <Button layoutX="199.0" layoutY="459.0" mnemonicParsing="false" onAction="#doSave" style="-fx-background-color: black;" text="Save" textFill="WHITE">
         <font>
            <Font name="Cambria Bold" size="20.0" />
         </font>
      </Button>
      <ComboBox fx:id="comboPropertyID" layoutX="29.0" layoutY="221.0" onAction="#docomboProperty" prefWidth="150.0" />
   </children>
</AnchorPane>
