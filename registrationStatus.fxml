<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="600.0" minWidth="650.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="registrationStatus.registrationStatusController">
   <children>
      <Label layoutX="198.0" layoutY="14.0" prefHeight="46.0" prefWidth="254.0" text="Find Property">
         <font>
            <Font name="Cambria Bold" size="38.0" />
         </font>
      </Label>
      <Label layoutX="31.0" layoutY="94.0" prefHeight="27.0" prefWidth="198.0" text="Registration Status">
         <font>
            <Font name="Cambria Bold" size="22.0" />
         </font>
      </Label>
      <RadioButton fx:id="radPending" layoutX="258.0" layoutY="96.0" mnemonicParsing="false" text="Pending">
         <font>
            <Font name="Cambria Bold" size="18.0" />
         </font>
         <toggleGroup>
            <ToggleGroup fx:id="status" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="radDone" layoutX="409.0" layoutY="99.0" mnemonicParsing="false" text="Done" toggleGroup="$status">
         <font>
            <Font name="Cambria Bold" size="18.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="radAll" layoutX="538.0" layoutY="99.0" mnemonicParsing="false" text="All" toggleGroup="$status">
         <font>
            <Font name="Cambria Bold" size="18.0" />
         </font>
      </RadioButton>
      <Label layoutX="31.0" layoutY="162.0" text="Registration After">
         <font>
            <Font name="Cambria Bold" size="22.0" />
         </font>
      </Label>
      <DatePicker fx:id="datePickerRegister" layoutX="251.0" layoutY="163.0" />
      <Button layoutX="485.0" layoutY="158.0" mnemonicParsing="false" onAction="#doShow" style="-fx-background-color: black;" text="Show" textFill="WHITE">
         <font>
            <Font name="Cambria Bold" size="18.0" />
         </font>
      </Button>
      <TableView fx:id="tblView" layoutX="24.0" layoutY="228.0" prefHeight="350.0" prefWidth="602.0" />
   </children>
</AnchorPane>
